  "vetur": {
    "tags": "vetur/rabbit-tags.json",
    "attributes": "vetur/rabbit-attributes.json"
  },
  "scripts": {
    "dev": "npm run gen:plugin-dev-route && vue-cli-service serve",
    "server": "node ./server.js",
    "build:dev": "cross-env RUN_ENV=dev vue-cli-service build",
    "build:test": "cross-env RUN_ENV=test vue-cli-service build",
    "build:prod": "cross-env RUN_ENV=prod vue-cli-service build",
    "build:plugin": "cross-env RUN_ENV=plugin vue-cli-service build && npm run pack:plugin-loader && npm run pack:plugin && npm run pack:gen-html && node scripts/generate-core-file.js && node scripts/generate-plugin-file.js && npm run pack:plugin-core",
    "preview": "node ./server.js",
    "lint": "eslint --ext .js,.vue src",
    "lint-fix": "eslint --fix --ext .js,.vue src",
    "svgo": "svgo -f src/assets/rabbit/icons/svg --config=src/assets/rabbit/icons/svgo.yml",
    "gen:plugin-dev-route": "node scripts/generate-plugin-route.js",
    "pack:app": "npm run clean && webpack --env NODE_ENV=production --config scripts/webpack.common.js",
    "pack:plugin": "rimraf lib/plugins && rimraf lib/plugin-hash.txt && webpack --env NODE_ENV=production --config scripts/webpack.plugin.js",
    "pack:plugin-loader": "webpack --env NODE_ENV=production --config scripts/webpack.plugin.rabbit.js",
    "pack:plugin-core": "rollup --config scripts/rollup.plugin.core.js",
    "pack:gen-html": "cross-env RUN_ENV=plugin node scripts/generate-html.js",
    "lib": "npm run pack:app && npm run pack:plugin-loader && npm run pack:plugin && tsc",
    "gen:component-entry": "node ./scripts/generate-component-entry.js",
    "clean": "rimraf lib types"
  },
  "keywords": [
    "vue",
    "admin",
    "management-system"
  ],
